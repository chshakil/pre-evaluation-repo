package com.example.student.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class Academic {

	@Id
	private int id;
	private String degree;
	private String sub;

	public Academic() {

	}

	public Academic(int id, String degree, String sub) {
		this.id = id;
		this.degree = degree;
		this.sub = sub;
	}

	public Academic(String degree, String sub) {
		super();
		this.degree = degree;
		this.sub = sub;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getDegree() {
		return degree;
	}

	public void setDegree(String degree) {
		this.degree = degree;
	}

	public String getSub() {
		return sub;
	}

	public void setSub(String sub) {
		this.sub = sub;
	}
}
